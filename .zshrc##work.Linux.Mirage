# If you come from bash you might have to change your $PATH.
export PATH=$HOME/.npm-global/bin/:$HOME/.local/bin:$PATH

source $HOME/.zshrc-generic

export SSH_ASKPASS=ksshaskpass

eval $(keychain --eval --quiet id_rsa ft_dev_rsa github_rsa)
alias init-socks-tunnel="screen -d -m -S stun socks-tunnel"
alias init-cors-anywhere="screen -d -m -S corsa cors-anywhere"
source /usr/share/nvm/init-nvm.sh

